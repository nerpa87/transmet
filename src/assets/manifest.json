{
	"manifest_version": 2,
	"name": "transmet",
	"version": "0.1",
	"description": "Inline translate tool",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": ["http://*/*"],
			"css": ["cs.css"],
			"js": ["cs.js"]
		}
	]
}
